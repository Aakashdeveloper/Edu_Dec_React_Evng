a.map((data) => {return data*1})
(6) [767, NaN, 1, NaN, 867, 7]
var num = [0,1,2,3]
num.map((data) => {return data*1})
(4) [0, 1, 2, 3]
num.filter((data) => {return data*1})
(3) [1, 2, 3]

