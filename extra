// Es5 
var    => Redeclare and reassign

// Es6
let   => We cannot Redeclare but we can reassign
const  => We cannot Redeclare nor reassign


var a  = 10


var a = declare
a = 10 => assignment


//Es5
function add(a,b){
    return a+b
}

//Es6
let add = (a,b) => { return a+b }

